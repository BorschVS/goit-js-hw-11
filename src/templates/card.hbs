{{#each articles}}
  <div class='photo-card'>
    <a href='{{largeImageURL}}'>
      <div class='thumb'>
        <img src='{{webformatURL}}' alt='{{tags}}' loading='lazy' />
      </div>
    </a>
    <div class='info'>
      <p class='info-item'>
        Likes
        <b>{{likes}}</b>

      </p>
      <p class='info-item'>
        Vievs
        <b>{{views}}</b>

      </p>
      <p class='info-item'>
        Comments
        <b>{{comments}}</b>

      </p>
      <p class='info-item'>
        Downloads
        <b>{{downloads}}</b>

      </p>
    </div>
  </div>
{{/each}}